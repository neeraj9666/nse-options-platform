{
    "name":  "nse-options-orchestrator",
    "version":  "1.0.1",
    "private":  true,
    "description":  "Electron + Vite + React NSE Options Platform",
    "main":  "server/dist/main.js",
    "scripts":  {
                    "dev":  "concurrently \"npm run build:watch\" \"npm run electron\"",
                    "build":  "tsc -p tsconfig.json",
                    "build:watch":  "tsc -p tsconfig.json --watch",
                    "electron":  "wait-on http://localhost:5173 \u0026\u0026 electron ."
                },
    "devDependencies":  {
                            "concurrently":  "^8.2.2",
                            "wait-on":  "^7.2.0",
                            "typescript":  "^5.3.3",
                            "electron":  "^28.0.0",
                            "vite":  "^5.4.0"
                        },
    "dependencies":  {
                         "dotenv":  "^16.4.5",
                         "pg":  "^8.11.3",
                         "zod":  "^3.22.4",
                         "axios":  "^1.6.0"
                     }
}
